{
  "name": "resi-script-manager",
  "version": "0.0.0",
  "description": "A manager for my resi-scripts.",
  "scripts": {
    "rollup-build": "rollup -c rollup.config.js",
    "rollup-dev": "rollup -c -w rollup.config.js",
    "gulp-build": "gulp build",
    "gulp-dev": "gulp dev",
    "build": "rollup-build && gulp-build",
    "dev": "npm run gulp-dev && npm run rollup-dev",
    "start": "sirv build",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile build/theme/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile build/theme/smui-dark.css -i src/theme/dark",
    "setup": "node setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheKeineAhnung/resi-script-manager.git"
  },
  "author": "KeineAhnung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheKeineAhnung/resi-script-manager/issues"
  },
  "homepage": "https://github.com/TheKeineAhnung/resi-script-manager#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@material/dom": "^13.0.0",
    "@material/theme": "^13.0.0",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@smui/button": "^6.0.0-beta.14",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.17",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-minify": "^3.1.0",
    "gulp-plumber": "^1.2.1",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.7",
    "postcss-preset-env": "^7.4.1",
    "rollup": "^2.68.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^2.0.2",
    "smui-theme": "^6.0.0-beta.14",
    "svelte": "^3.46.4",
    "svelte-preprocess": "^4.10.4",
    "yargs": "^17.3.1"
  },
  "dependencies": {}
}
