    "@types/jquery": "3.5.27",
    "@types/leaflet": "1.9.8",
    "@types/node": "18.14.6",
    "@types/yargs": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "autoprefixer": "10.4.16",
    "axios": "1.6.1",
    "copy-webpack-plugin": "11.0.0",
    "cssnano": "5.1.15",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-svelte3": "4.0.0",
    "eslint-plugin-unicorn": "45.0.2",
    "fancy-log": "2.0.0",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-plumber": "1.2.1",
    "material-colors": "1.2.6",
    "postcss": "8.4.31",
    "postcss-preset-env": "7.8.3",
    "prettier": "2.8.4",
    "rollup": "2.61.1",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-inject-process-env": "1.3.1",
    "rollup-plugin-svelte": "7.1.6",
    "sass": "1.58.3",
    "smui-theme": "7.0.0-beta.0",
    "svelte": "3.55.1",
    "svelte-preprocess": "4.10.7",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "webpack": "5.76.0",
    "webpack-cli": "5.0.1",
    "webpack-stream": "7.0.0",
    "yargs": "17.7.2"
  },
  "scripts": {
    "build": "node --loader=ts-node/esm --experimental-specifier-resolution=node --no-warnings build.ts --cmd='build'",
    "dev": "node --loader=ts-node/esm --experimental-specifier-resolution=node --no-warnings build.ts --cmd='dev'",
    "gulp-build": "gulp build --mode='development'",
    "gulp-build:prod": "gulp build --mode='production'",
    "gulp-build:beta": "gulp build --mode='beta'",
    "gulp-dev": "gulp dev --mode='development'",
    "gulp-dev:beta": "gulp dev --mode='beta'",
    "gulp-dev:production": "gulp dev --mode='production'",
    "theme": "npm run smui-theme-light && npm run smui-theme-dark",
    "rollup-build": "rollup -c rollup.config.js --mode='development'",
    "rollup-build:prod": "rollup -c rollup.config.js --mode='production'",
    "rollup-build:beta": "rollup -c rollup.config.js --mode='beta'",
    "rollup-dev": "rollup -c -w rollup.config.js --mode='development'",
    "rollup-dev:beta": "rollup -c -w rollup.config.js --mode='beta'",
    "rollup-dev:prod": "rollup -c -w rollup.config.js --mode='production'",
    "smui-theme-dark": "smui-theme compile build/theme/smui-dark.css -i src/theme/dark",
    "smui-theme-light": "smui-theme compile build/theme/smui.css -i src/theme",
    "update": "npx npm-check -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheKeineAhnung/resi-script-manager.git"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "singleQuote": true,
    "spaceBeforeFunctionParen": true,
    "arrowParens": "avoid",
    "endOfLine": "crlf"
  },
  "keywords": [
    "Rettungssimulator",
    "skripte",
    "skript-manager"
  ],
  "license": "CC-BY-NC-SA-4.0"
}
